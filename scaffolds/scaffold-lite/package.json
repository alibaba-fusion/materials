{
  "name": "@alifd/scaffold-lite",
  "version": "0.3.0",
  "description": "轻量级模板，使用 TypeScript，仅包含基础的 Layout。",
  "files": [
    ".vscode/",
    "src/",
    "build/",
    "public/",
    "tests/",
    "_gitignore",
    "_editorconfig",
    "_eslintignore",
    "_eslintrc.js",
