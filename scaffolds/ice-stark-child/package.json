    "@types/react": "^16.9.20",
    "@types/react-dom": "^16.9.5",
    "axios": "^0.17.1",
    "moment": "^2.24.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  },
  "devDependencies": {
    "@ice/spec": "^1.0.0",
    "build-plugin-fusion": "^0.1.0",
    "build-plugin-icestark": "^1.0.2",
